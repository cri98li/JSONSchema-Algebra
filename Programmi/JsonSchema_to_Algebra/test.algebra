rootdef "root" = {
	type[
		arr
	],
	items[
		{
			type[
				obj
			],
			props[
				"^relayId$": type[
					str
				],
				"^data$": {
					type[
						obj
					],
					props[
						"^[0-9a-zA-Z_-]{1,255}$": type[
							num,
							str,
							bool
						];
						false
					]
				},
				"^time$": type[
					str
				];
				false
			],
			req[
				"data"
			]
		};
		
	]
}