{
    "configuration": {
        "status": "info",
        "ThresholdFilter": {
            "level": "off"
        },
        "appenders": {
            "Console": {
                "name": "console",
                "PatternLayout": {
                    "pattern": "[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %.-100m%n"
                }
            }
        },
        "loggers": {
            "root": {
                "level": "all",
                "includeLocation": "true",
                "AppenderRef": {
                    "ref": "console"
                }
            }
        }
    }
}