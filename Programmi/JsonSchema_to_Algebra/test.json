    "share-parcel": {
      "type": "object",
      "properties": {
        "end_date": {
          "format": "date",
          "type": "string"
        },
        "percentage_of_shares_max": {
          "maximum": 100,
          "minimum": 0,
          "type": [
            "number",
            "null"
          ]
        },
        "percentage_of_shares": {
          "maximum": 100,
          "minimum": 0,
          "type": "number"
        },
        "voting_percentage": {
          "maximum": 100,
          "minimum": 0,
          "type": "number"
        },
        "percentage_of_shares_min": {
          "maximum": 100,
          "minimum": 0,
          "type": [
            "number",
            "null"
          ]
        },
        "shareholders": {
          "minItems": 1,
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "name"
            ],
            "properties": {
              "identifier": {
                "type": "string"
              },
              "company_number": {
                "type": "string"
              },
              "address": {
                "ref": "#/definitions/address"
              },
              "address_country": {
                "type": "string"
              },
              "jurisdiction": {
                "type": "string"
              },
              "name": {
                "minLength": 1,
                "type": "string"
              },
              "type": {
                "enum": [
                  "Company",
                  "Person"
