        "legal_form": {
          "type": "string"
        },
        "industry_codes": {
          "type": "array",
          "items": {
            "ref": "#/definitions/industry-code"
          }
        },
        "mailing_address": {
          "ref": "#/definitions/address"
        },
        "name": {
          "minLength": 1,
          "type": "string"
        },
        "other_attributes": {
          "type": "object"
        },
        "previous_names": {
          "type": "array",
          "items": {
            "ref": "#/definitions/previous-name"
          }
        }
