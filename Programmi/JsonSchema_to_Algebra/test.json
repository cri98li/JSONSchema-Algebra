{
  "$defs": {
    "date": {
      "type": "object",
      "properties": {
        "month": {
          "type": "number"
        },
        "year": {
          "type": "number"
        },
        "day": {
          "type": [
            "number",
            "null"
          ]
        }
      },
      "additionalProperties": false
    },
    "identifier": {
      "type": "object",
      "properties": {
        "uid": {
          "minLength": 1,
          "type": "string"
        },
        "identifier_system_code": {
          "type": "string"
        }
      },
      "required": [
        "uid",
        "identifier_system_code"
      ]
    },
    "address": {
      "oneOf": [
        {
          "minLength": 2,
          "type": [
            "string",
            "null"
          ]
        },
        {
          "allOf": [
            {
              "required": [
                "street_address"
              ]
            },
            {
              "required": [
                "postal_code"
              ]
            },
            {
              "required": [
                "locality",
                "country"
              ]
            }
          ],
          "type": "object",
          "properties": {
            "street_address": {
              "type": "string"
            },
            "country": {
              "type": "string"
            },
            "country_code": {
              "type": "string"
            },
            "locality": {
              "type": "string"
            },
            "region": {
              "type": "string"
            },
            "postal_code": {
              "type": "string"
            }
          }
        }
      ]
    },
    "unknown-entity-type": {
      "type": "object",
      "properties": {
        "website": {
          "oneOf": [
            {
              "minLength": 5,
              "type": "string"
            },
            {
              "type": [
                "array",
                "null"
              ],
              "items": {
                "minLength": 5,
                "type": "string"
              }
            }
          ]
        },
        "addresses": {
          "type": "array",
          "items": {
            "ref": "#/definitions/address-with-type"
