          "minLength": 1,
          "type": "string"
        },
        "name": {
          "minLength": 1,
          "type": "string"
        },
        "all_attributes": {
          "type": "object",
          "properties": {
            "merged_into": {
              "type": "object",
              "properties": {
                "effective_date": {
                  "format": "date",
                  "type": "string"
                },
                "surviving_company": {
                  "allOf": [
                    {
                      "required": [
                        "name"
                      ]
                    },
                    {
                      "required": [
                        "company_number"
                      ]
                    }
                  ],
                  "type": "object",
                  "properties": {
                    "company_number": {
                      "minLength": 1,
                      "type": "string"
                    },
                    "jurisdiction": {
                      "minLength": 2,
                      "type": "string"
                    },
                    "name": {
                      "minLength": 1,
                      "type": "string"
                    }
                  }
                }
              },
              "additionalProperties": false,
              "required": [
                "surviving_company"
              ]
            },
            "number_of_employees": {
              "allOf": [
                {
                  "minLength": 1,
                  "type": "string"
                },
                {
                  "minimum": 0,
                  "type": "number"
