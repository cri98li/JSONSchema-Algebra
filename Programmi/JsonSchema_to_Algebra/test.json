          }
        },
        "position": {
          "type": "string"
        },
        "start_date": {
          "allOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "name"
      ]
    },
    "organisation": {
      "type": "object",
      "properties": {
        "website": {
          "oneOf": [
            {
              "minLength": 5,
