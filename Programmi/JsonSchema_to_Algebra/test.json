        },
        "registered_address": {
          "ref": "#/definitions/address"
        },
        "headquarters_address": {
          "ref": "#/definitions/address"
        },
        "identifiers": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "ref": "#/definitions/identifier"
          }
        },
        "jurisdiction": {
          "type": "string"
        },
        "telephone_number": {
          "minLength": 4,
          "type": "string"
        },
        "fax_number": {
          "minLength": 4,
          "type": "string"
        },
        "alternative_names": {
          "type": "array",
          "items": {
            "ref": "#/definitions/alternative-name"
          }
        },
        "industry_codes": {
          "type": "array",
          "items": {
            "ref": "#/definitions/industry-code"
          }
        },
        "mailing_address": {
          "ref": "#/definitions/address"
        },
        "uid": {
          "type": "string"
        },
        "name": {
          "minLength": 1,
          "type": "string"
        },
        "other_attributes": {
          "type": "object"
        },
        "previous_names": {
          "type": "array",
          "items": {
            "ref": "#/definitions/previous-name"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "name"
      ]
    },
    "total-shares": {
      "type": "object",
      "properties": {
        "number": {
          "type": "integer"
        },
        "share_class": {
          "minLength": 1,
          "type": "string"
        }
      },
      "required": [
        "number"
      ]
    },
    "officer": {
      "type": "object",
      "properties": {
        "end_date": {
          "allOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "uid": {
          "type": "string"
        },
        "name": {
          "not": {
            "pattern": "^[\\.\\,\\*\\\\\\-\\s\\{\\}\\(\\)]+$"
          },
          "minLength": 1,
          "type": "string"
        },
        "other_attributes": {
          "type": "object",
          "properties": {
            "address": {
              "ref": "#/definitions/address"
            },
            "nationality": {
              "type": "string"
            },
            "date_of_birth": {
              "format": "date",
              "type": "string"
